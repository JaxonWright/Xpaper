<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- Device Models Available-->
    <string-array name="models_array">
        <item>Moto X Pure Edition</item>
        <item>Moto X Style</item>
        <item>Moto X Play</item>
        <item>Moto X Force</item>
        <item>Moto X 2014</item>
        <item>Moto X 2013</item>

    </string-array>

    <!-- Wallpaper Category / Theme names-->
    <!-- use '#' at beginning to signify as premium-->
    <string-array name="categories">
        <item>Basic X</item>
        <item>Wing</item>
        <item>VVV</item>
        <item>Backslash</item>
        <!--<item>#Premium Test Cat</item>-->
    </string-array>

    <!-- Category thumbnail previews-->
    <array name="cat_thumbs">
        <item>@drawable/basicx</item>
        <item>@drawable/wingcat</item>
        <item>@drawable/vvvcat</item>
        <item>@drawable/backslashcat</item>
        <!--<item>@drawable/testcatblank</item>-->
    </array>

    <array name="cat_foregrounds">
        <item>@drawable/basic_foreground</item>
        <item>@drawable/wing_foreground</item>
        <item>@drawable/vvv_foreground</item>
        <item>@drawable/backslash_foreground</item>
        <!--<item>@drawable/basic_foreground</item>-->
    </array>

    <array name="pure_textures">
        <item>@drawable/texture_bamboo</item>
        <item>@drawable/texture_bamboo_icon</item>
        <item>@drawable/texture_ebony</item>
        <item>@drawable/texture_ebony_icon</item>
        <item>@drawable/texture_charcoal_ash</item>
        <item>@drawable/texture_charcoal_ash_icon</item>
    </array>

    <!-- Color options for wallpaper editing-->
    <string-array name="color_option_array">
        <item>Front Color</item>
        <item>Back Color</item>
        <item>Accent Color</item>
